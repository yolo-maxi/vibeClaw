{
  "name": "agentation",
  "version": "1.4.0",
  "description": "Visual feedback for AI coding agents - with API mode and multiplayer support",
  "license": "PolyForm-Shield-1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "dev": "pnpm build && pnpm watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "agentation",
    "feedback",
    "annotations",
    "ai",
    "claude",
    "coding-agent",
    "visual-feedback",
    "multiplayer"
  ],
  "homepage": "https://github.com/yolo-maxi/agentation",
  "repository": {
    "type": "git",
    "url": "https://github.com/yolo-maxi/agentation.git"
  },
  "bugs": {
    "url": "https://github.com/yolo-maxi/agentation/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.0",
    "esbuild-sass-plugin": "^3.6.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.5.6",
    "postcss-modules": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.97.2",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
